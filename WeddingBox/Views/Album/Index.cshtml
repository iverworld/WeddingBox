@{
    ViewBag.Title = "Index";
}

<div class="container">
    <div class="row">
        @foreach (var filename in Model.filePaths)
        {
            <div class="col-md-3 col-sm-4 col-xs-6">
                <img src='@Url.Action(filename)' class="img-responsive">
            </div>
        }        
    </div>
</div>